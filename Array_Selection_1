import java.util.Scanner;
public class Array_Selection_1{
public static void main(String[]args){
Scanner sc = new Scanner (System.in);
int i = 0;
double a[] = new double[5];
System.out.println("Enter Number : ");
for( i = 0 ; i<5 ; i++){
a[i] = sc.nextDouble();
}
for( i = 0 ; i<5 ; i++){
if (a[i]<=10)
System.out.println("a["+i+"] = "+a[i]);
}
}
}

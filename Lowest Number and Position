import java.util.Scanner;
public class Uri{
public static void main(String[] args){
Scanner sc = new Scanner(System.in);
System.out.print("Enter n : ");
int n = sc.nextInt();
int i = 0;
int lowest  = 1;
int position = 0;
int a[] = new int[n];
for( i = 0 ; i< n ; i++){
a[i] = sc.nextInt();
if(a[i] < lowest)
lowest = a[i];
}
System.out.println("Lowest : " +lowest);
//System.out.println("Position : "+position);
}
}


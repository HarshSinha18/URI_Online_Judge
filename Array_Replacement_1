import java.util.Scanner;
public class Array {
public static void main(String[] args){
Scanner sc = new Scanner(System.in);
int i = 0;
int x[] = new int[10];
System.out.println("Enter Numbers: ");
for(i = 0 ; i<10 ; i++){
x[i] = sc.nextInt();
if(x[i]== 0 )
x[i] = 1;
if(x[i]<0)
x[i] = 1;
}
for(i =0;i <10; i++){
System.out.println("a["+i+"] = "+x[i]);
}
}
}

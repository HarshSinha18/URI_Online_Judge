import java.util.Scanner;
public class Array_Fill{
public static void main(String[] args){
Scanner sc = new Scanner(System.in);
int i =0;
System.out.print("Enter Number for First Position : ");
int num = sc.nextInt();
int a[]  = new int[10];
for (i = 0 ; i<10 ; i++){
a[0] = num;
a[i]= i*2;
}
for(i = 0 ; i<10 ; i++){
System.out.println("a["+i+"] = "+a[i]);
}
}
}
